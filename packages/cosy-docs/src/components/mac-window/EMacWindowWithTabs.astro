---
/**
 * @component MacWindow.WithTabs
 *
 * @description
 * 带标签页的MacWindow组件示例，展示标签页功能。
 */
import { MacWindow } from '@coffic/cosy-ui/vue';
---

<MacWindow
  title="设置"
  tabs={['通用', '外观', '高级']}
  defaultTab="外观"
  onTabClick={(tab: string) => {
    console.log('切换到标签:', tab);
  }}>
  <div class="cosy:p-4">
    <div>设置内容区域</div>
  </div>
</MacWindow>
