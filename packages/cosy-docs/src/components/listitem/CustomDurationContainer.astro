---
import { CodeContainer } from '@coffic/cosy-ui';
import CustomDuration from './CustomDuration.astro';
import CustomDurationSource from './CustomDuration.astro?raw';
import CustomDurationVue from './CustomDuration.vue';
import VueSource from './CustomDuration.vue?raw';
---

<CodeContainer
  codes={[CustomDurationSource, VueSource]}
  titles={['Astro', 'Vue']}>
  <div id="tab-1" class="not-prose">
    <CustomDuration />
  </div>
  <div id="tab-2" class="not-prose">
    <CustomDurationVue client:idle />
  </div>
</CodeContainer>
