---
import { CodeContainer } from '@coffic/cosy-ui';
import Basic from './Basic.astro';
import BasicVue from './Basic.vue';
import BasicSource from './Basic.astro?raw';
import VueSource from './Basic.vue?raw';
---

<CodeContainer codes={[BasicSource, VueSource]} titles={['Astro', 'Vue']}>
  <div id="tab-1" class="not-prose">
    <Basic />
  </div>
  <div id="tab-2" class="not-prose">
    <BasicVue client:idle />
  </div>
</CodeContainer>
