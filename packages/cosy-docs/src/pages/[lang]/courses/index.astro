---
import BaseLayout from '@/layouts/BaseLayout.astro';
import { LinkUtil } from '@coffic/cosy-ui';

export const getStaticPaths = async () => {
  return [
    {
      params: { lang: 'zh-cn' },
      props: {
        lang: 'zh-cn',
      },
    },
    {
      params: { lang: 'en' },
      props: {
        lang: 'en',
      },
    },
  ];
};

const redirectLink = LinkUtil.createUrl('/en/courses/guide');
return Astro.redirect(redirectLink);
---

<BaseLayout title="Courses" />
