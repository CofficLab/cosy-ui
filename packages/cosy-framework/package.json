  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "keywords": [
    "framework",
    "typescript",
    "astro",
    "laravel",
    "web-framework"
