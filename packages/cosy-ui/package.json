{
    "name": "@coffic/cosy-ui",
    "version": "0.5.2",
    "description": "An astro component library",
    "author": {
        "name": "nookery",
        "url": "https://github.com/nookery"
    },
    "repository": {
        "url": "https://github.com/CofficLab/cosy-ui"
    },
    "license": "MIT",
    "keywords": [
        "astro-integration",
        "astro-component",
        "withastro",
        "astro",
        "cosy-ui"
    ],
    "homepage": "https://cofficlab.github.io/cosy-ui/en/",
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist",
        "index.ts"
    ],
    "scripts": {
        "dev": "astro dev --host 0.0.0.0 --port 5678",
        "preview:docs": "astro preview --host 0.0.0.0 --port 4330 --outDir dist-docs",
        "preview": "npm run preview:docs",
        "build": "vite build && npm run build:docs && tsx scripts/post-build.ts",
        "build:docs": "astro build"
    },
    "type": "module",
    "peerDependencies": {
        "astro": "^5.1.3"
    },
    "dependencies": {
        "astro-integration-kit": "^0.18.0",
        "fs-extra": "^11.3.0"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.4",
        "@astrojs/mdx": "^4.2.0",
        "@astrojs/ts-plugin": "^1.10.4",
        "@astrojs/vue": "^5.0.8",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.4",
        "@types/chai": "^5.2.0",
        "@types/eslint": "^9.6.1",
        "@types/fs-extra": "^11.0.4",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.13.10",
        "@types/react": "^19.0.10",
        "@typescript-eslint/parser": "^8.26.1",
        "astro": "^5.5.2",
        "chai": "^4.5.0",
        "daisyui": "^5.0.4",
        "eslint": "^8.57.1",
        "eslint-plugin-astro": "^1.3.1",
        "mocha": "^10.8.2",
        "prettier": "^3.5.3",
        "prettier-plugin-astro": "^0.14.1",
        "rollup-plugin-copy": "^3.5.0",
        "sharp": "^0.33.2",
        "tailwindcss": "^4.1.4",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vite": "^6.2.2",
        "vue": "^3.5.13"
    }
}
