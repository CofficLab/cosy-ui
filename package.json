{
  "name": "@coffic/cosy-ui",
  "version": "0.1.26",
  "description": "A astro component library",
  "repository": {
    "url": "https://github.com/CofficLab/cosy-ui"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.ts",
  "exports": {
    ".": "./index.ts"
  },
  "files": [
    "src",
    "index.ts"
  ],
  "keywords": [
    "astro-component"
  ],
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint . --ext .ts,.js,.astro src",
    "dev": "astro dev --host 0.0.0.0 --root example",
    "build": "astro build --root example",
    "preview": "astro preview --root example"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.14",
    "@types/chai": "^5.2.0",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.26.1",
    "astro": "^5.5.2",
    "chai": "^4.5.0",
    "daisyui": "^5.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "mocha": "^10.8.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.33.2",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "astro": "^5.0.0",
    "daisyui": "^5.0.3"
  },
  "dependencies": {
    "@astrojs/vue": "^5.0.7",
    "@remixicon/vue": "^4.6.0",
    "prismjs": "^1.29.0",
    "vue": "^3.5.13"
  }
}
