---
import AstroIcon from './AstroIcon.astro';

interface Props {
	/**
	 * 图标的大小
	 * @default "24px"
	 */
	size?: string;
	/**
	 * 自定义类名
	 */
	class?: string;
}

const { size = '24px', class: className = '' } = Astro.props;
---

<AstroIcon name="github" size={size} fill={true} stroke="none" class={className} />
