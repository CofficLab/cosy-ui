{
    "extends": "astro/tsconfigs/base",
    "include": [
        ".astro/types.d.ts",
        "index.ts",
        "src"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "~/*": [
                "./*"
            ],
            "~": [
                "./"
            ]
        },
        "plugins": [
            {
                "name": "@astrojs/ts-plugin"
            }
        ]
    }
}