{
    "extends": "astro/tsconfigs/strict",
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist",
        "dist-docs"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@cosy/*": [
                "./src/*"
            ],
            "@docs/*": [
                "./docs/*"
            ]
        },
        "jsx": "preserve"
    }
}